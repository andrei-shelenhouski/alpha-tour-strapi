{
  "kind": "collectionType",
  "collectionName": "configs",
  "info": {
    "singularName": "config",
    "pluralName": "configs",
    "displayName": "Config"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "configs"
    },
    "navigation": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "global.navigation",
      "repeatable": false
    },
    "footer": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "global.footer",
      "repeatable": false
    },
    "company": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "yearOfEstablishment": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "2002",
      "regex": "\\d{4}$"
    },
    "showInternetAcquiring": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "internetAcquiring": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "showInternetAcquiring"
            },
            true
          ]
        }
      },
      "enum": [
        "webpay",
        "bepaid",
        "strapi"
      ]
    },
    "showPhoneNumber": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "phoneNumber": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "showPhoneNumber"
            },
            true
          ]
        }
      }
    },
    "showSubscriptionForm": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "subscriptionForm": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "showSubscriptionForm"
            },
            true
          ]
        }
      },
      "component": "global.subscription-form",
      "repeatable": false
    },
    "showFooterAddressBlock": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "footerAddressBlock": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "showFooterAddressBlock"
            },
            true
          ]
        }
      }
    },
    "companyRegistrationDetails": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "showInternetAcquiring"
            },
            true
          ]
        }
      }
    },
    "instagramProfileLink": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.link",
      "repeatable": false
    },
    "facebookProfileLink": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.link",
      "repeatable": false
    },
    "showAssociation": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "associationLogo": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "showAssociation"
            },
            true
          ]
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "associationLink": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "showAssociation"
            },
            true
          ]
        }
      },
      "component": "shared.link",
      "repeatable": false
    }
  }
}
