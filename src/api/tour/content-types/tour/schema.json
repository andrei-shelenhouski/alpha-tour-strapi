{
  "kind": "collectionType",
  "collectionName": "tours",
  "info": {
    "singularName": "tour",
    "pluralName": "tours",
    "displayName": "Tour"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "country": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "customField": "plugin::country-select.country"
    },
    "place": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::place.place",
      "inversedBy": "tours"
    },
    "transport_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::transport-type.transport-type"
    },
    "page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::universal.universal",
      "inversedBy": "tours"
    },
    "departure_airport": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::airport.airport"
    },
    "meal_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::meal-type.meal-type"
    },
    "room_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::room-type.room-type"
    },
    "adults": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "children": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "price": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "currency": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "BYN",
      "enum": [
        "PLN",
        "EUR",
        "USD",
        "BYN",
        "GEL"
      ]
    },
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 1000
    },
    "note": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 200
    },
    "internal_ref": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "exchange_rate_multiplier": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": 3
    },
    "duration": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": 0
    },
    "start_date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "end_date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "effective_date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "expiry_date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "service_note": {
      "type": "text",
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 200
    },
    "config": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::config.config",
      "inversedBy": "tours"
    },
    "room_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::room-category.room-category"
    },
    "thumbnail": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
